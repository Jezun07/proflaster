{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/pro-plaster.myshopify.com/products/configure/181486/edit
{% endcomment %}
{% if product.metafields.custom_fields["taping"] != blank %}
  <div class="custom-field custom-field__taping custom-field__type--checkbox">
    <h3 class="custom-field--title">Taping</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["taping"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}